{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "requireConfigFile": false
    },
    "rules": {
        "no-unused-vars" : "warn",
        "no-extra-semi" : "warn",
        "no-mixed-spaces-and-tabs" : "warn"
    }
}
